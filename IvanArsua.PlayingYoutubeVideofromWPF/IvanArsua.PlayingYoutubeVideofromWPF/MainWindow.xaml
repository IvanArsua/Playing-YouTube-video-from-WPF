        <TextBox x:Name="txtVideoUrl" HorizontalAlignment="Left" Height="36" Margin="28,26,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="738"/>
        <WebBrowser x:Name="Browser" HorizontalAlignment="Left" Height="404" Margin="28,80,0,0" VerticalAlignment="Top" Width="738"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Left" Height="31" Margin="598,504,0,0" VerticalAlignment="Top" Width="168" Click="btnGo_Click"/>
    </Grid>
